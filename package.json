{
  "name": "corona-api",
  "version": "1.0.0",
  "description": "data API",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format":"echo 'RUNNING PRITTIER'; prettier --write '**/*.{js,yml,md}'",
    "start": "npm run format && echo 'FETCHING DATA'; node fetch.js && node formatGlobal.js && npm run archive",
    "archive": "node save.js",
    "purge-archive": "rm -rf ./Data/archive/* ",
    "purge-json": "rm -r ./Data/data_sets/*",
    "purge-csv": "rm -r ./Data/raw/*",
    "purge-all": " npm run purge-json; npm run purge-csv; npm run purge-archive"
  },
  "keywords": [
    "coronavirus",
    "covid-19",
    "API",
    "Dataset"
  ],
  "author": "Tomaszito",
  "license": "ISC",
  "dependencies": {
    "csv-parse": "^4.8.8",
    "csv2json": "^2.0.2",
    "download-file": "^0.1.5",
    "node-fetch": "^2.6.0"
  }
}
